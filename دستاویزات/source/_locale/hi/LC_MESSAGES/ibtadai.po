#: /home/travis/build/julienmalard/Taqdir/دستاویزات/source/ibtadai.rst:62
msgid ""
"from تقدیر.مقام import مقام\n"
"from تقدیر.ذرائع import سی_اس_وی\n"
"\n"
"میرا_مقام = مقام(‏عرض=11.02, طول=76.96, بلندی=1)\n"
"مشاہدات = سی_اس_وی('مشاہدات.csv', عرض=11.02, طول=76.96, بلندی=1)\n"
"کو = میرا_مقام.کوائف_پانا(سے='۲۰۱۸۰۱۰۱', تک='۲۰۱۷۰۱۰۱', ذرائع=مشاہدات)"
msgstr ""
"from تقدیر.مقام import مقام as जगह\n"
"from تقدیر.ذرائع import سی_اس_وی as सी_एस_वी\n"
"\n"
"मेरा_जगह = जगह(چوڑائی=11.02, طول=76.96, بلندی=1)\n"
"प्रे = सी_एस_वी('प्रेक्षण.csv', چوڑائی=11.02, طول=76.96, بلندی=1)\n"
"आँकड़े = मेरा_जगह.کوائف_پانا(سے='२०१८०१०१', تک='२०१७०१०१', ذرائع=प्रे)"

